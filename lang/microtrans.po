#: microtrans.py:53
#, python-format
msgid "\tUsage: %s -lvhKkqd\n"
msgstr "\tИспользуйте: %s -lvhKkqd\n"

#: microtrans.py:54
msgid ""
"\tWhere:\n"
"    \t -l - translation mode (en-ru, de-en...);\n"
"\t -v - displays version information;\n"
"\t -h - displays help;\n"
"    \t -k - set a key (k=key);\n"
"    \t -K - set key from file (K=filename);\n"
"\t -d - dialog-mode;\n"
"\t -q - hide more information;\n"
msgstr ""
"\tГде:\n"
"\t -l - режим перевода (en-ru, de-en и т.д.);\n"
"\t -v - выводит версию программы;\n"
"\t -h - выводит справку;\n"
"\t -k - задаёт ключ (k=ключ);\n"
"\t -K - задаёе ключ из файла (K=имя-файла);\n"
"\t -d - режим диалога;\n"
"\t -q - скрыть дополнительную информацию;\n"

